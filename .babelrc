{
    "presets": [
      ["env", {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }],
      "stage-2"
    ],
    "plugins": ["transform-runtime",
          ["prismjs", {
            "languages": ["javascript", "typescript", "markup"],
            "plugins": ["line-numbers"],
            "theme": "Dark",
            "css": true
          }]
      ]
  }